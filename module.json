{
	"name":"LS Wing MIDI",
	"type":"MIDI",
	"path":"MIDI Control",
	
	"version":"1.0",
	"description":"Special MIDI Module LS Wing controlers in MIDI-Mode",
	"url":"",
	"downloadURL":"",
	
	"hasInput":true,
	"hasOutput":true, 

	"hideDefaultCommands":false,



	"parameters":
	{
	
		
	},

	"defaults":
	{		
		"devices":["LS Wing","LS Wing"],
		"autoAdd":false
	},

	"hideDefaultParameters":
	[
		"autoAdd",
		"autoFeedback",
		"useHierarchy"
	],


	"scripts":
	[
		"lswing_midi.js" ],

	"values":
	{ },
	
	
"commands":
	{	
	
	"Set Color" : {
			"menu" : "Control",
			"callback" : "set_color",
			"parameters" : { 
					
				"Select Row (1-6)": {
					"type": "Enum",
					"options" : {
						"1- Row1" : 0,
						"2- Row2" :1,
						"3- Select" : 3,
						"4- Play" :4,
						"5- Pause" : 5,
						"6- Mute/Flash" :2  } },
					
				"Button N° (1-10)": {
					"type": "Integer",
					"default": 1, 
					"min":1,
					"max":10,
					"useForMapping": true },  
					
				"Choose Color": {
					"type": "Enum",
					"options" : {
						"Off" : 0,
						"Red" :20,
						"Green" : 40,
						"Blue" :60,
						"Yellow" : 70,
						"Magenta" :90,
						"Cyan" : 110,
						"White" :120  } }		
			} },
				
	
	"Send ControlChange" : {
			"menu" : "Control",
			"callback" : "send_control",
			"parameters" : {
			
			"MIDI Channel": {
					"type": "Integer",
					"default": 1, 
					"min":1,
					"max":16,
					"useForMapping": true },
					
			"Set Controler N°": {
					"type": "Integer",
					"default": 1, 
					"min":0,
					"max":127,
					"useForMapping": true },
					
			"Set Value": {
					"type": "Integer",
					"default": 1, 
					"min":0,
					"max":127,
					"useForMapping": true }
		}  },
		
	"Send NoteOn" : {
			"menu" : "Control",
			"callback" : "send_note",
			"parameters" : {
			
			"MIDI Channel": {
					"type": "Integer",
					"default": 1, 
					"min":1,
					"max":16,
					"useForMapping": true },
					
			"Set Note N°": {
					"type": "Integer",
					"default": 1, 
					"min":0,
					"max":127,
					"useForMapping": true },
					
			"Set Velocity": {
					"type": "Integer",
					"default": 1, 
					"min":0,
					"max":127,
					"useForMapping": true }
		}  },
		
	"Reset All Colors" : {
			"menu" : "Control",
			"callback" : "reset_colors"
		   },
		
		
		
} }
